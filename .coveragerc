[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */.venv/*
    */__pycache__/*
    */node_modules/*
    */static/*
    */media/*
    manage.py
    */settings/*
    */tests/*
    */test_*
    */conftest.py
    */pytest.ini
    */requirements.txt
    */README.md
    */docs/*
    */scripts/*
    */tools/*
    */deployment/*
    */config/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2
