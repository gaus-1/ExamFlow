{
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.git",
    "**/venv",
    "**/.venv",
    "**/env",
    "**/.env",
    "**/migrations",
    "**/static",
    "**/media",
    "**/docs/_build",
    "**/.pytest_cache",
    "**/coverage",
    "**/htmlcov",
    "**/dist",
    "**/build",
    "**/*.egg-info"
  ],
  "ignore": [
    "**/migrations/**",
    "**/static/**",
    "**/media/**"
  ],
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic"
}